{
  "name": "@powcaptcha/signals-analyzer",
  "version": "1.0.2",
  "license": "MIT",
  "description": "PowCaptcha Signals Analyzer - A library for analyzing PowCaptcha signals.",
  "keywords": [
    "powcaptcha",
    "fingerprint",
    "captcha",
    "security",
    "anti-bot"
  ],
  "type": "module",
  "exports": {
